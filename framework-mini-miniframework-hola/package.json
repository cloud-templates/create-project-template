{
  "name": "hola-template",
  "version": "0.0.1",
  "private": true,
  "description": "mini program app",
  "templateInfo": {
    "name": "taro-ui-vue",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "prepare": "npm run format",
    "format": "prettier --write \"src/**/*.{js,jsx,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "build:gmump": "light release --type gmump",
    "build:gmuapp": "light release --type gmuapp",
    "build:alipay": "taro build --type alipay",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:gmump": "light release --type gmump --watch",
    "dev:gmuapp": "light release --type gmuapp --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "build:swan": "taro build --type swan",
    "build:tt": "taro build --type tt",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "winner-fed",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@holajs/hola": "latest",
    "@holajs/plugin-platform-gmump": "^0.1.5",
    "@holajs/preset-gmu": "latest",
    "@tarojs/cli": "3.3.15",
    "@tarojs/components": "3.3.15",
    "@tarojs/plugin-framework-vue2": "3.4.2",
    "@tarojs/runtime": "3.3.15",
    "@tarojs/taro": "3.3.15",
    "@winner-fed/cloud-utils": "^1.1.1",
    "@winner-fed/magicless": "*",
    "lodash": "4.17.15",
    "taro-ui-vue": "^1.0.0-alpha.5",
    "vue": "^2.5.0",
    "vue-template-compiler": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.3.15",
    "@tarojs/webpack-runner": "3.3.15",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.x",
    "@typescript-eslint/parser": "^2.x",
    "babel-preset-taro": "3.3.15",
    "colors": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.1.4",
    "eslint-plugin-vue": "^7.x",
    "fs-extra": "^10.0.1",
    "minimist": "^1.2.5",
    "prettier": "^2.4.1",
    "shelljs": "^0.8.5",
    "stylelint": "9.3.0",
    "typescript": "^3.7.0",
    "vue-loader": "^15.9.2"
  }
}
